{
  "variables": {
    "virtualbox_headless": "true",
    "virtualbox_memory": "8192",
    "virtualbox_cpus": "4",
    "virtualbox_boot_wait": "5s",
    "virtualbox_communicator": "ssh",
    "virtualbox_communicator_username": "vagrant",
    "virtualbox_communicator_password": "vagrant",
    "virtualbox_communicator_timeout": "15m",
    "virtualbox_guest_additions_mode": "disable",
    "virtualbox_shutdown_command": "echo 'vagrant' | sudo -S shutdown -P now"
  }
}