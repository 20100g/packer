{
  "variables": {
    "vmware_headless": "true",
    "vmware_memory": "8192",
    "vmware_cpus": "4",
    "vmware_boot_wait": "5s",
    "vmware_communicator": "ssh",
    "vmware_communicator_username": "vagrant",
    "vmware_communicator_password": "vagrant",
    "vmware_communicator_timeout": "15m",
    "vmware_shutdown_command": "echo 'vagrant' | sudo -S shutdown -P now"
  }
}